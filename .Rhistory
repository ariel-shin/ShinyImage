shinyimg <- R6Class("shinyimg",
public = list(
brightness = 0,
contrast = 0,
actions = 0,
xy1 = c(0, 0),
xy2 = NULL,
img_history = c(),
local_img = NULL,
current_img = NULL,
initialize = function(inputImage) {
self$local_img <- readImage(inputImage)
self$current_img <- self$local_img * 1
self$xy2 <- c(dim(self$local_img)[1], dim(self$local_img)[2])\
print(.self)
}
)
)
